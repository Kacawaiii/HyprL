api.hyprlcore.com {
    reverse_proxy localhost:8080
    
    header {
        Access-Control-Allow-Origin *
    }
}

app.hyprlcore.com {
    reverse_proxy localhost:8501
}

hyprlcore.com, www.hyprlcore.com {
    root * /opt/hyprl/apps/landing
    file_server
    
    handle /api/* {
        reverse_proxy localhost:8080
    }
}